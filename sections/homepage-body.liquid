<div class="discount">
  <div class="discount__background">
    <p class="discount__title">SEASON CLERANCE</p>
    <p class="discount__text">ALL KITS<br>50% OFF</p>
  </div>
</div>

<div class="sailors-and-pirates">
  <div class="sailors-and-pirates__background"></div>
  <div class="sailors-and-pirates__content">
    <p class="sailors-and-pirates__content--title">SAILORS & PIRATES KIT</p>
    <p class="sailors-and-pirates__content--text">This awesome kit includes ennui, church-key organic small batch kitsch
      selfies ugh PBR Godard.</p>
    <button class="sailors-and-pirates__content--button">SEE KIT</button>
  </div>
</div>

<div class="archeologist">
  <div class="archeologist__background"></div>
  <div class="archeologist__content">
    <p class="archeologist__content--title">ARCHEOLOGIST KID</p>
    <p class="archeologist__content--text">This awesome kit includes ennui, church-key organic small batch kitsch
      selfies ugh PBR Godard.</p>
    <button class="archeologist__content--button">SEE KIT</button>
  </div>
</div>

<div " class=" featured-toys">

  {%- assign collection = collections[section.settings.collection] -%}
  <div class="section-header text-center">
    <h1 class="featured-toys__title">{{ collection.title }}</h1>
    {%- assign product_limit = collection.products_count -%}
  </div>

  <ul class="featured-products collection-slider">

    {% for product in collection.products %}
    <li class="featured-products__item">
      <img class="featured-products__item--image" src="{{product.featured_image | img_url: '300x300'}}" alt="">
      <span class="featured-products__item--title">{{product.title}}</span>
      <h1 class="featured-products__item--price">${{product.price }}USD</h1>
      <a href="{{ product.url }}">
        <button class="featured-products__item--button">VIEW
        </button>
      </a>
    </li>
    {% endfor %}
  </ul>
</div>

<div class="wooden-toys">
  <div class="wooden-toys__background"></div>
  <div class="wooden-toys__content"></div>
  <p class="wooden-toys__content--title">WOODEN TOYS</p>
</div>

<div class="plush-toys">
  <div class="plush-toys__background"></div>
  <div class="plush-toys__content"></div>
  <p class="plush-toys__content--title">PLUSH TOYS</p>
</div>

<div class="outdoors">
